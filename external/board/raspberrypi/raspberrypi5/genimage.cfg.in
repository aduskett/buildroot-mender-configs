image boot.vfat {
  vfat {
    files = {
      "u-boot.bin",
      "Image",
      "bcm2712-rpi-5-b.dtb",
      "rpi-firmware/cmdline.txt",
      "rpi-firmware/config.txt",
      "#OVERLAY_DIR#",
    }
  }
  size = 64M
}

image DEVICE_TYPE {
  hdimage {}

  partition boot {
    partition-type = 0xC
    bootable = "true"
    image = "boot.vfat"
  }

  partition rootfsa {
    partition-type = 0x83
    image = "rootfs.ext4"
  }

  partition rootfsb {
    partition-type = 0x83
    image = "rootfs.ext4"
  }

  partition data {
    partition-type = 0x83
    image = "data-part.ext4"
  }

}
